#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules/@n8n/scan-community-package/scanner/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules/@n8n/scan-community-package/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules/@n8n/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules/@n8n/scan-community-package/scanner/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules/@n8n/scan-community-package/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules/@n8n/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/@n8n+scan-community-package@0.1.1/node_modules:/Users/eliottcerpaud/Desktop/n8n-nodes-linkup/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@n8n/scan-community-package/scanner/cli.mjs" "$@"
else
  exec node  "$basedir/../@n8n/scan-community-package/scanner/cli.mjs" "$@"
fi
